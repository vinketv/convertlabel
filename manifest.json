{
    "manifest_version": 3,
    "name": "Convert Label",
    "version": "1.0.1",
    "description": "Download and crop PDF Vinted Label for your Thermal Printer",
    "author": "Vinke",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "activeTab",
        "downloads"
    ],
    "host_permissions": [
		"file:///*",
		"*://*/*"
	],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options/index.html"
}